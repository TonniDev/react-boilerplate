{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate",
  "main": "./src/index.js",
  "scripts": {
    "start": "clear && npm run dev",
    "dev": "NODE_ENV=development ROOT_PATH=./ HOST=localhost PORT=8090 webpack --display-error-details -w --config webpack.config.js --hot --mode development",
    "build": "ROOT_PATH=./ HOST=localhost OUTPUT=standard PORT=8111 npm run build:prod",
    "build:prod": "npm update coi-minhaoi && npm run build:envConfig && ROOT_PATH=./ webpack --display-error-details --config webpack.config.js -p --mode production",
    "test": "node private/jest/test.js --env=jsdom",
    "test:watch": "npm test -- --watch",
    "test:coverage": "jest --coverage",
    "coverage": "npm test -- --coverage",
    "storybook": "start-storybook -p 6009 -c private/storybook",
    "build-storybook": "build-storybook -c private/storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/private/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/storybook.js",
      "<rootDir>/private/jest/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules|private|test)/"
    ],
    "testEnvironment": "node",
    "cacheDirectory": "test/.cache",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "test/.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "globals": {},
    "notify": false,
    "resetMocks": false
  },
  "author": "TonniDev",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "create-react-class": "^15.6.3",
    "dotenv": "^7.0.0",
    "format-currency-to-br": "^1.0.0",
    "history": "^4.9.0",
    "moment": "^2.24.0",
    "polished": "^3.1.0",
    "react": "^16.8.5",
    "react-code-splitting": "^1.2.1",
    "react-cookie": "3.1.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.5",
    "react-file-download": "^0.3.5",
    "react-google-tag-manager": "^2.2.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.8.0",
    "react-npm": "^2.6.1",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-slick": "^0.23.2",
    "react-transition-group": "^2.7.1",
    "redux": "^4.0.1",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6",
    "styled-tools": "^1.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
