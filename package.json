    "prop-types": "^15.7.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^8.0.0",
    "react-test-renderer": "^16.8.5",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-devtools-log-monitor": "^1.4.0",
